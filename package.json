{
  "name": "rollup-plugin-markdown",
  "version": "0.2.0",
  "description": "Extract arbitrary code from Markdown code blocks",
  "main": "dist/markdown.js",
  "module": "dist/markdown.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevonCash/rollup-plugin-markdown.git"
  },
  "keywords": [
    "Markdown",
    "documentation",
    "literate",
    "programming",
    "writing"
  ],
  "scripts": {
    "lint": "eslint --config eslintrc.json ./source/*.js && eslint --config eslintrc.json ./test/*.js",
    "test": "mocha --require reify test/*.js",
    "build": "rollup --config rollup.config.js",
    "prepublish": "git checkout ./source/markdown.js && yarn build && yarn lint && yarn test"
  },
  "author": "Vijith Assar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevonCash/rollup-plugin-markdown/issues"
  },
  "homepage": "https://github.com/DevonCash/rollup-plugin-markdown#readme",
  "dependencies": {
    "magic-string": "^0.22.4",
    "rollup-pluginutils": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "mocha": "^4.0.1",
    "reify": "^0.12.3",
    "rollup": "^1.1.2",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
